<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".mainHome">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="40dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/holder"
            android:layout_width="397dp"
            android:layout_height="90dp"
            android:layout_marginTop="715dp"
            android:background="@drawable/bottombox"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/homemain2"
                android:layout_width="101dp"
                android:layout_height="match_parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/selecthome" />

            <ImageView
                android:id="@+id/add2"
                android:layout_width="101dp"
                android:layout_height="match_parent"
                app:layout_constraintStart_toEndOf="@+id/homemain2"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/hanap" />

            <ImageView
                android:id="@+id/message"
                android:layout_width="101dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:layout_constraintStart_toEndOf="@+id/add2"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/message" />

            <ImageView
                android:id="@+id/prof2"
                android:layout_width="101dp"
                android:layout_height="match_parent"
                app:layout_constraintStart_toEndOf="@+id/message"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/profile"
                tools:srcCompat="@drawable/profile" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="65dp"
                android:text="HOME"
                android:textColor="#33443C"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="124dp"
                android:layout_marginTop="65dp"
                android:text="SEARCH"
                android:textColor="#33443C"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="220dp"
                android:layout_marginTop="65dp"
                android:text="MESSAGES"
                android:textColor="#33443C"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView32"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="325dp"
                android:layout_marginTop="65dp"
                android:text="PROFILE"
                android:textColor="#33443C"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainerView"
        android:name="com.egls.transactia.hfrag"
        android:layout_width="397dp"
        android:layout_height="764dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_hfrag" />
</androidx.constraintlayout.widget.ConstraintLayout>